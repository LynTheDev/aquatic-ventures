frame = 0

minutes = 0
seconds = 0

function update_game()

 move()
 p_boundary()

 update_bubbles()
 update_enemies()
 
 frame += 1 
 if frame == 30 then
  frame = 0
  seconds += 1
  
  if seconds == 60 then
   seconds = 0
   minutes += 1
  end
 end
end

function draw_game()
 cls(12)
 map(0, 0, 0, 0, 128, 128)
 
 print(minutes .. ":" .. seconds, 5, 5, 7)

 spr(p.sp, p.x, p.y, 1, 1, p.flipped)
 
 if p.is_debug then
  print("x: " .. p.x .. " y: " .. p.y, 5, 5, 7)
  print(h_seconds .. " " .. h_minutes)
 
  rect(p.box.x, p.box.y, p.box.x + p.box.w, p.box.y + p.box.h, 7)
 end
 
 draw_bubbles()
 draw_enemies()
end
