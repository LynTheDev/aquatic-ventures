function _init()
 spawn_bubbles()
 spawn_enemies()
 
 cartdata("omonattie_aquaticventures")
 
 h_seconds = dget(0)
 h_minutes = dget(1)
end

function _update()
 if state == states.main_menu then
  update_menu()
 end

 if state == states.playing then
  update_game()
 end
 
 if state == states.dead then
  update_death()
 end
end

function _draw()
 if state == states.main_menu then
  draw_menu() 
 end

 if state == states.playing then
  draw_game()
 end
 
 if state == states.dead then
  draw_death()
 end
 
 if state == states.box_art then
  draw_ba()
 end
end

function is_colliding(b1, b2)
 ol_min_x = max(b1.x, b2.x)
 ol_min_y = max(b1.y, b2.y)
 
 ol_max_x = min(b1.x + b1.w, b2.x + b1.w)
 ol_max_y = min(b1.y + b1.h, b2.y + b1.h)

 if ol_min_x <= ol_max_x and ol_min_y <= ol_max_y then
  return true 
 end
 
 return false
end

function save()
 dset(0, h_seconds)
 dset(1, h_minutes)
end
