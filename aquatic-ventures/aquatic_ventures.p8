orig_x = flr(rnd(100))
orig_y = flr(rnd(80))

p = {
 is_debug = false,
 
 x = orig_x,
 y = orig_y,
 
 vx = 0,
 vy = 0,
 
 speed = 2,
 
 size = 0,
 
 sp = 1,
 fliped = false,
 
 box = {
  x = orig_x,
  y = orig_y,
  
  h = 4,
  w = 6
 }
}

function move()
 p.vx = 0
 p.vy = 0
 
 if btn(0) then
  p.flipped = true
  p.vx -= p.speed 
 end
 
 if btn(1) then
  p.flipped = false
  p.vx += p.speed 
 end
 
 if btn(2) then
  p.vy -= p.speed 
 end
 
 if btn(3) then
  p.vy += p.speed 
 end

 p.x += p.vx
 p.y += p.vy
 
 p.box.x = p.x
 p.box.y = p.y + 2
end

function p_boundary()
 if p.y <= 30 then
  p.y = 30
  p.box.y = 30 + 2
 end
 
 if p.y >= 122 then
  p.y = 122
 end
 
 if p.x <= -6 then
  p.x = 120
 end
 
 if p.x >= 126 then
  p.x = -5
 end
end

