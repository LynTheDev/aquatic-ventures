bubbles = {}

bd = {
 sp_count = 3,
 sp_start = 57,
 
 move = 1
}

function spawn_bubbles()
 for i = 1, 7 do 
  bubble = {
   x = flr(rnd(120)),
   y = 130 + flr(rnd(10)),
   
   speed = flr(rnd(15)) + 25,
   death = 30 + flr(rnd(70)),
   
   sp = flr(rnd(bd.sp_count) + bd.sp_start)
  }
  
  add(bubbles, bubble)
 end
end

function update_bubbles()
 bd.move -= 1
 
 if bd.move == 0 then
  if count(bubbles) > 1 then
   for b in all(bubbles) do
    b.y -= ((b.y + 20) - b.y) / b.speed 
   end
  end
  
  bd.move = 1
 end

 for b in all(bubbles) do
  if b.y <= b.death then
   del(bubbles, b)
   sfx(0)
  end
 end
 
 if count(bubbles) <= flr(rnd(4)) then
  spawn_bubbles()
 end
end

function draw_bubbles()
 for b in all(bubbles) do
  spr(b.sp, b.x, b.y) 
 end
end
