enemies = {}

f_sc = 3
enemie_count = 7

function spawn_enemies()
 for i = 1, enemie_count do
  local rng = flr(rnd(2))
  
  if rng == 1 then
   e_flip = false
   
   local x_f = 8 + flr(rnd(70))
   f_x = -x_f
  else 
   e_flip = true
   f_x = 130 + flr(rnd(100))
  end
  
  f_y = 30 + flr(rnd(90))
  f_type = 1 + flr(rnd(5))
  sp_s = 0
  
  if f_type == 1 then
   sp_s = 128
  elseif f_type == 2 then
   sp_s = 144
  elseif f_type == 3 then
   sp_s = 160
  elseif f_type == 4 then
   sp_s = 176
  elseif f_type == 5 then
   sp_s = 148
  end
  
  ssize = flr(rnd(3)) + 0
  
  if ssize == 0 then
   sp_s = sp_s
  elseif ssize == 1 then
   sp_s = sp_s + 1
  elseif ssize == 2 then
   sp_s = sp_s + 2
  end
    
  fish = {
   x = f_x,
   y = f_y,
   
   is_flipped = e_flip,
   
   size = 0 + flr(rnd(4)),
   
   speed = flr(rnd(10)) + 20,
   
   size = ssize,
   sp = sp_s,
   
   box = {
    x = f_x,
    y = f_y + 2,
    
    w = 6,
    h = 4
   }
  }
  
  add(enemies, fish)
 end
end

function update_enemies()
 for f in all(enemies) do
  if f.x < -40 then
   del(enemies, f)
  end
  
  if f.x > 200 then
   del(enemies, f)
  end
  
  if is_colliding(p.box, f.box) then
   
   sfx(1)
   
   if minutes > h_minutes then
    h_minutes = minutes
    h_seconds = seconds
   end
   
   if minutes >= h_minutes
   and seconds > h_seconds
   then
    h_minutes = minutes
    h_seconds = seconds
   end
   
   save()
   
   state = states.dead
  end
 end
 
 if count(enemies) < 10 then
  spawn_enemies()
  enemie_count += 5
 end

 if count(enemies) > 1 then
  for f in all(enemies) do
   if f.is_flipped then
    f.x -= ((f.x + 30) - f.x) / f.speed
   else
    f.x += ((f.x + 30) - f.x) / f.speed   
   end
   
   f.box.x = f.x
   f.box.y = f.y + 2
  end
 end
end

function draw_enemies()
 for f in all(enemies) do 
  if p.is_debug then
   rect(f.box.x, f.box.y, f.box.x + f.box.w, f.box.y + f.box.h, 7)
   
   if f.size >= 2 then
    print(f.size)
   end
  end
 
  spr(f.sp, f.x, f.y, 1, 1, f.is_flipped)
 end
 
 if p.is_debug then
  print(count(enemies), 5, 30, 7)
 end
end
